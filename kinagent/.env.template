# Azure Container Registry Configuration
ACR_REGISTRY=obscr.azurecr.io
ACR_REPO=kagent-dev/kagent
ACR_USERNAME=your-acr-username
ACR_PASSWORD=your-acr-password

# AKS Deployment Configuration
AKS_NAMESPACE=kagent
AKS_SERVICE_TYPE=ClusterIP
AKS_DEFAULT_MODEL_PROVIDER=azureOpenAI

# Azure OpenAI Configuration
AZUREOPENAI_API_KEY=your-azure-openai-api-key
AZUREOPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZUREOPENAI_DEPLOYMENT=your-deployment-name
AZUREOPENAI_API_VERSION=2024-08-01-preview
AZUREOPENAI_MODEL=gpt-4o

# OpenTelemetry Configuration
OTEL_ENDPOINT=http://otel-collector.merlin-dev.svc.cluster.local:4317

# Optional: Other LLM Provider API Keys (only needed if using those providers)
# OPENAI_API_KEY=your-openai-api-key
# ANTHROPIC_API_KEY=your-anthropic-api-key
# GOOGLE_API_KEY=your-google-api-key

# Databricks DataSource Configuration (optional)
# When set, a DataSource CR and credentials Secret will be created automatically
DATABRICKS_TOKEN=your-databricks-personal-access-token
DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
DATABRICKS_CATALOG=main
DATABRICKS_SCHEMA=
DATABRICKS_WAREHOUSE_ID=
